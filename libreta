def imprimir(n, tipo):
      
    if n <= 4:
        print("El valor de n debe ser mayor que 4")
    

    elif tipo == 1:
            print("n" + "".join(["*" + str(n)] * n))


    elif tipo == 2:

        print("n" + "" .join(["*" + str(n)] * n))

        for i in range (n - 2):
                print("*" + "" * (n -2) + "*")

    elif tipo == 3:
         print("n" + "".join(["*" + str(n)] * n))

         for i in range (1, n + 1):
                print("" * (n - i) + "*" * (2* i - 1 ))
